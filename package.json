{
  "name": "crimson-shinobi",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "capacitor:init": "npx cap init 'Crimson Shinobi' 'com.shinobi.crimson'",
    "capacitor:assets": "npx @capacitor/assets generate --iconPath resources/icon.png --splashPath resources/splash.png",
    "capacitor:add-platforms": "npx cap add android && npx cap add ios",
    "capacitor:sync": "npm run build && npx cap sync",
    "capacitor:open:android": "npx cap open android",
    "capacitor:open:ios": "npx cap open ios"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@capacitor/android": "^5.5.1",
    "@capacitor/assets": "^3.0.4",
    "@capacitor/cli": "^5.5.1",
    "@capacitor/core": "^5.5.1",
    "@capacitor/ios": "^5.5.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-pwa": "^0.16.7"
  }
}